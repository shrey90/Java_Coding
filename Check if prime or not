import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
         int t = sc.nextInt();
         int count = 0;
         for(int i=1;i<=t;i++){
             if(t%i==0){
                 count++;
             }
         }if(count ==2){
             System.out.println("YES");
         }else{
             System.out.println("NO");
         }
    }
}
