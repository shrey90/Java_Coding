
import java.util.*;

    public class Main {
        static int factorial(int n){
        int ans = 1;
        for(int i = 1;i<=n;i++){
            ans = ans * i;
        }
        return ans;
    }
        public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
            System.out.println(factorial(5));
            int n = scn.nextInt();
            int fact= factorial(n);
            System.out.println(fact);



    }
}
